                if ($('.continue_tutorial a img').attr('src') == 'images/finish.png') {
                    $('.continue_tutorial a img').on('click', function () {
                        $('.onload_tutotial').hide();
                        $('.additional_options span').trigger('click');
                    })
                }
            })
            $('.onload_tutotial span').on('click', function () {
                $('.onload_tutotial').hide();
            })
