 $.ajax({
        type: "POST",
        url: "login.php",
        data: "check=login",
        async: false,
        success: function (response) {
            var res = response.split("##");
            var app_id = res[1];
            var autherId = res[0];
            if (autherId != '') {
			$('.screen_popup').fadeIn();
			$('.popup_container').fadeIn();

