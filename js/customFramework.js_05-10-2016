    function nextImage(index) {
        $('.pager div').removeClass('active');
        $('.pager div').eq(index).addClass('active');

        $('.banner img').hide().eq(index).show();
    }


    setInterval(function () {
        newindex = newindex + 1;
        if (newindex == 3) {
            newindex = 0;
        }
        nextImage(newindex);

    }, 2000);


