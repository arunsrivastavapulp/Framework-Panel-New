            name: 'New Screen',
            icon: '',
            originalIndex: newindex+deletedScreenCount,
            keyword: ''
        });
        if($("#present").length!=0)
        {
        	 $("#present").attr("data-link", newindex);
        	$("#present").attr("data-originalIndex",newindex+deletedScreenCount)
        }
         $(".save_hidden").trigger("click");
          savehtml();
