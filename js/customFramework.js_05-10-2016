	$('input.sh-checkbox').on('change', function () {  
  if ($(this).is(':checked')) {    
    $('.sh-hide-one').hide();    
    $('#present').attr('all-day-event', '1');  
  }  
  else {    
    $('.sh-hide-one').show();    
    $('#present').attr('all-day-event', '0');  
  }
});
